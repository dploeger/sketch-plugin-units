  "devDependencies": {
    "@skpm/builder": "^0.5.11"
  },
  "author": "Dennis Ploeger <develop@dieploegers.de>",
  "dependencies": {
    "sketch-module-web-view": "^2.1.6",
    "prettier": "1.16.4"
  }
}
